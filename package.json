{
  "version": "1.0.0",
  "scripts": {
    "commit": "git-cz",
    "changelog": "conventional-changelog.cmd -p atom -i CHANGES.md -s -r 0"
  },
  "devDependencies": {
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0"
  },
  "config": {
    "commitizen": {
